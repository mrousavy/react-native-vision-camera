{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docs": "typedoc",
    "build": "bun run docs && fumadocs-mdx && next build",
    "dev": "bun run docs && next dev",
    "start": "next start",
    "clean": "rm -rf .next .source content/api-reference",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@r4ai/remark-callout": "^0.6.2",
    "fumadocs-core": "16.1.0",
    "fumadocs-docgen": "^3.0.4",
    "fumadocs-mdx": "14.0.3",
    "fumadocs-typescript": "^4.0.13",
    "fumadocs-ui": "16.1.0",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "picocolors": "^1.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "ts-morph": "^27.0.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-frontmatter": "^1.1.0",
    "typescript": "^5.9.3"
  }
}