{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": [
    "camera"
  ],
  "ios": {
    "iosModuleName": "VisionCamera"
  },
  "android": {
    "androidNamespace": [
      "camera"
    ],
    "androidCxxLibName": "VisionCamera"
  },
  "autolinking": {
    "CameraFactory": {
      "swift": "HybridCameraFactory",
      "kotlin": "HybridCameraFactory"
    },
    "PreviewView": {
      "swift": "HybridPreviewView",
      "kotlin": "HybridPreviewView"
    },
    "FrameRendererView": {
      "swift": "HybridFrameRendererView",
      "kotlin": "HybridFrameRendererView"
    },
    "WorkletQueueFactory": {
      "cpp": "HybridWorkletQueueFactory"
    }
  },
  "ignorePaths": [
    "**/node_modules"
  ]
}
